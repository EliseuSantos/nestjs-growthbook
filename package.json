{
  "name": "nestjs-growthbook",
  "version": "1.0.0",
  "description": "Provides an injectable growthbook client to provide feature flag check on nestjs modules",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/EliseuSantos/nestjs-growthbook/issues"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EliseuSantos/nestjs-growthbook.git"
  },
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "commit": "cz",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish": "npx semantic-release --no-ci",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "prepare": "husky"
  },
  "keywords": [
    "nestjs",
    "growthbook"
  ],
  "author": "Eliseu",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/testing": "10.3.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.2",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@growthbook/growthbook": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}